{
  "presets": ["./scripts/babel-preset-es2015", "stage-1", "react"],
  "plugins": [
    "./scripts/babel-transform-object-assign-require"
  ],
  "env": {
    "coverage": {
      "plugins": [
        ["istanbul"]
      ]
    },
    "test": {
      "sourceMaps": "both"
    },
    "production": {
      "plugins": [
        "transform-dev-warning",
        "transform-runtime"
      ]
    }
  }
}
